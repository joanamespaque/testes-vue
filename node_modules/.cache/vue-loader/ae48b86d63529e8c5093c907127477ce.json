h3>Informações Pesssoais</h3> -->\n        <v-row>\n          <v-col>\n            <v-text-field\n              name=\"nome\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-account\"\n              :type=\"'text'\"\n              label=\"Nome Completo\"\n              :rules=\"[$store.state.rules.required,$store.state.rules.min]\"\n            ></v-text-field>\n          </v-col>\n          <the-mask :mask=\"['(##) ####-####', '(##) #####-####']\" />\n\n          <v-col>\n            <v-text-field\n              name=\"rg\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-card-account-details-outline\"\n              :type=\"'number'\"\n              label=\"RG\"\n              :rules=\"[$store.state.rules.required]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"cpf\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-id-card\"\n              :type=\"'text'\"\n              label=\"CPF\"\n              :rules=\"[$store.state.rules.required, maxfields(11), minfields(11)]\"\n            ></v-text-field>\n          </v-col>\n        </v-row>\n        <v-row>\n          <v-col>\n            <v-select\n              color=\"teal darken-2\"\n              prepend-inner-icon=\"mdi-gender-male-female\"\n              :items=\"sexos\"\n              :rules=\"[$store.state.rules.required]\"\n              label=\"Sexo\"\n            ></v-select>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"data_nascimneto\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-calendar-account\"\n              :type=\"'date'\"\n              label=\"Data de Nascimento\"\n              :rules=\"[$store.state.rules.required]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"salario\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-account-cash\"\n              :type=\"'number'\"\n              label=\"Salário\"\n              :rules=\"[$store.state.rules.required]\"\n            ></v-text-field>\n          </v-col>\n        </v-row>\n        <h3>Endereço</h3>\n        <v-row>\n          <v-col>\n            <v-text-field\n              name=\"cep\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-map-marker\"\n              :type=\"'number'\"\n              label=\"CEP\"\n              v-model=\"cep\"\n              v-on:keyup.native=\"preencherCep\"\n              :rules=\"[$store.state.rules.required, $store.state.rules.min,$store.state.rules.max ]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"rua\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              :type=\"'text'\"\n              label=\"Rua\"\n              prepend-inner-icon=\"mdi-sign-real-estate\"\n              id=\"rua\"\n              v-model=\"rua\"\n              :rules=\"[$store.state.rules.required]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"numero\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              :type=\"'number'\"\n              label=\"Número\"\n              prepend-inner-icon=\"mdi-home-floor-0\"\n              :rules=\"[$store.state.rules.required]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"complemento\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-domain\"\n              :type=\"'text'\"\n              label=\"Complemento\"\n            ></v-text-field>\n          </v-col>\n        </v-row>\n        <v-row>\n          <v-col>\n            <v-text-field\n              name=\"bairro\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              :type=\"'text'\"\n              label=\"Bairro\"\n              prepend-inner-icon=\"mdi-home-group\"\n              id=\"bairro\"\n              v-model=\"bairro\"\n              :rules=\"[$store.state.rules.required]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"cidade\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              :type=\"'text'\"\n              label=\"Cidade\"\n              prepend-inner-icon=\"mdi-city\"\n              id=\"cidade\"\n              v-model=\"cidade\"\n              :rules=\"[$store.state.rules.required]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-select\n              name=\"estado\"\n              label=\"Estado\"\n              color=\"teal darken-2\"\n              id=\"estado\"\n              v-model=\"estado\"\n              :items=\"estados\"\n              item-text=\"nome\"\n              item-value=\"sigla\"\n              :rules=\"[$store.state.rules.required]\"\n            ></v-select>\n          </v-col>\n        </v-row>\n        <h3>Informações da Conta</h3>\n        <v-row>\n          <v-col>\n            <v-text-field\n              name=\"email\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-email\"\n              :type=\"'email'\"\n              label=\"E-mail\"\n              :rules=\"[$store.state.rules.email, $store.state.rules.required]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"senha\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-lock\"\n              :type=\"'password'\"\n              label=\"Senha\"\n              v-model=\"senha\"\n              :rules=\"[$store.state.rules.required, $store.state.rules.min]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-text-field\n              name=\"senha\"\n              color=\"teal darken-2\"\n              class=\"input-change\"\n              prepend-inner-icon=\"mdi-lock\"\n              :type=\"'password'\"\n              label=\"Confirmar Senha\"\n              v-model=\"confirmaSenha\"\n              :rules=\"[combinaSenha, $store.state.rules.required, $store.state.rules.min]\"\n            ></v-text-field>\n          </v-col>\n          <v-col>\n            <v-switch\n              color=\"teal darken-2\"\n              class=\"ma-4\"\n              label=\"Tornar Administrador\"\n              name=\"administrador\"\n              v-model=\"administrador\"\n            ></v-switch>\n          </v-col>\n        </v-row>\n        <div class=\"buttons\">\n          <v-btn :disabled=\"!valid\" color=\"teal lighten-1\" class=\"mr-4\" @click=\"validate\">Cadastrar</v-btn>\n\n          <v-btn color=\"yellow darken-3\" dark @click.stop=\"confirmaReset = true\">Resetar</v-btn>\n\n          <v-dialog v-model=\"confirmaReset\" max-width=\"360\">\n            <v-card>\n              <v-card-title class=\"headline\">Você tem certeza que deseja resetar esse formulário?</v-card-title>\n\n              <v-card-text>Ao concordar, todos os campos do formulário serão limpos.</v-card-text>\n\n              <v-card-actions>\n                <v-spacer></v-spacer>\n\n                <v-btn\n                  color=\"teal lighten-1 \"\n                  text\n                  @click=\"confirmaReset = false, reset()\"\n                >Concordar</v-btn>\n\n                <v-btn color=\"blue-grey lighten-1\" text @click=\"confirmaReset = false\">Cancelar</v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n        </div>\n      </v-form>\n    </v-app>\n  </v-card-text>\n</template>\n\n<script>\nimport { getCep, getEstados } from \"@/services.js\";\nimport { mapFields } from \"@/helpers.js\";\nimport { mask } from \"vue-the-mask\";\nexport default {\n  name: \"CadastroFuncionario\",\n  directives: { mask },\n  computed: {\n    combinaSenha() {\n      const rule = v => (!!v && v) === this.senha || \"As senhas não combinam\";\n      return rule;\n    },\n    ...mapFields({\n      fields: [\n        \"nome\",\n        \"sexo\",\n        \"celular\",\n        \"data_nascimneto\",\n        \"cpf\",\n        \"rg\",\n        \"salario\",\n        \"email\",\n        \"rua\",\n        \"numero\",\n        \"complemento\",\n        \"cep\",\n        \"numero\",\n        \"bairro\",\n        \"cidade\",\n        \"estado\",\n        \"administrador\",\n        \"senha\",\n        \"confirmaSenha\"\n      ],\n      base: \"usuario\",\n      mutation: \"UPDATE_USUARIO\"\n    })\n  },\n  data: () => ({\n    sexos: [\"Feminino\", \"Masculino\"],\n    valid: true,\n    select: null,\n    estados: [],\n    confirmaReset: false,\n    mask: [\"(##) #####-####\", \"(##) ####-####\"]\n  }),\n  methods: {\n    validate() {\n      this.$refs.form.validate();\n    },\n    reset() {\n      this.$refs.form.reset();\n    },\n    preencherCep() {\n      const cep = this.cep.replace(/\\D/g, \"\");\n      if (cep.length === 8) {\n        getCep(cep).then(response => {\n          if (!response.data.erro) {\n            this.rua = response.data.logradouro;\n            this.bairro = response.data.bairro;\n            this.estado = response.data.uf;\n            this.cidade = response.data.localidade;\n          }\n        });\n      }\n    },\n    maxfields(max) {\n      const rule = v =>\n        v ? v.length <= max || `Máximo ${max} caracteres` : \"Preenchido\";\n      return rule;\n    },\n    minfields(min) {\n      const rule = v =>\n        v ? v.length >= min || `Mínimo ${min} caracteres` : \"Preenchido\";\n      return rule;\n    },\n    onInput(formattedNumber, { number, valid, country }) {\n      this.phone.number = number.international;\n      this.phone.valid = valid;\n      this.phone.country = country && country.name;\n    },\n    validaNumero() {\n      const rule = v =>\n        v ? !this.phone.isValid || \"O número precisa ser válido.\" : \"Validado\";\n      console.log(\"oi\");\n      return rule;\n    }\n  },\n  created() {\n    getEstados().then(r => {\n      let data = r.data.sort(function(a, b) {\n        return comparaStrings(a.nome, b.nome);\n      });\n      this.estados = data;\n    });\n  }\n};\n\nfunction comparaStrings(a, b) {\n  a = a.toLowerCase();\n  b = b.toLowerCase();\n  return a < b ? -1 : a > b ? 1 : 0;\n}\n</script>\n\n<style scoped>\n</style>"]}]}